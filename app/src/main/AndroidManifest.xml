<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthyfoodapp">

    <application
        android:icon="@mipmap/ic_ikonica1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ikonica1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WriteYourRecipeActivity" />
        <activity android:name=".Favorites" />
        <activity android:name=".DetailRecipeActivity" />
        <activity android:name=".ListOfRecipes" />
        <activity android:name=".RecipesCategoryActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>